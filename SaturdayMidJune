pipeline {
    agent any
    stages { 
    	stage('source') {
	steps {
	 echo 'try and put something here first'	
	     }
	}
	tage('compile') {
	steps {
	    echo 'try and compile for for experiments'
		}
	}
    
        stage('Build') {
            steps {
                echo 'Hello World'
		echo 'saturday trial'
            }
        }
	stage('Test') {
          environment {
          OUTPUT_PATH = './outputs/'
      }
	steps {
	    echo 'In the testing phase'
	    echo 'passive trial'
       	}
      }
      stage('Predeploy') {
      	   input {
            message 'Predeploy?'
	    ok 'proceed'
	    }
	    steps {
                echo "Testing release"
            }
        }
       stage('Deploy') {
	steps {
	    echo 'In the testing phase'
	    echo 'active deployment'
       	}
      }
      stage('Monitor') {
      	steps {
	    echo 'In the testing phase'
	    echo 'intensive monitoring'
       	}
      }
    }
}
