pipeline {
	agent any
	stages {
		stage ('build') {
    steps {
      echo 'test integration'
    }
		}
		stage ('approval') {
    steps {
      echo 'test approval'
    }
		}
		stage ('deploy:prod') {
    steps {
      sh'''
      rm -rf testfile.json
      '''
      echo 'test deploy'
    }
		}
	}
}
