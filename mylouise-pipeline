
pipeline {
    agent any
    stages { 
        stage('Clone') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Prebuild') {
            steps {
            echo 'Get things ready'  
            }
        }
	stage('Build') {
	   environment {
	   LOG_LEVEL='INFO'
	   }
	   parallel {
	   stage('linux-arm64') {
	       steps {
	        echo "Building release..."
	       }
	   }
	   stage('windows-amd64') {
	   	   }

}
	
}
    }
}
