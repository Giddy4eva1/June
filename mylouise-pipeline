pipeline {
	agent any
	environment {

		RELEASE='2.04'
		
	}

	stages{
		
	stage('BUILD') {
		environment {
		   LOG_LEVEL='INFO'
}
	stage('Test') {
	input {
	message 'continue?'
	ok 'continue!'
	echo "testing the parameters"
	
}

	stage('Deploy') {
	input {
	message 'Deploy?'
	ok 'do it!'
}

}

}
	


}


}



}
