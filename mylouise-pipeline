pipeline {
    agent any
    stages { 
        stage('Build') {
            steps {
                echo 'Hello World'
		echo 'first build'
            }
        }
	stage('Pretest') {
	    steps {
	    echo "test this one now"
}
	    steps {
	    echo 'begin  something'
	    echo 'see stakeholders'

}
}     
    }
}
