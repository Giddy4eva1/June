pipeline {
    agent any                            
    environment {
       OUPUT_PATH = './outputs/'
        }
    stages {
        stage('Code Compile') {
           steps {
                echo 'Hello, Maven'
            }
        }
        stage('Code Build') {
           steps {
                echo 'Hello, JDK'
            }
        }
        stage('Code Review') {
           steps {
                echo 'Hello, JDK'
            }
        }
        stage('Code Analysis') {
          steps {
           input {
           message 'Deploy?'
           ok 'Do it!'
              
          }
        }
 
        }
        stage('Code Test') {
           steps {
                echo 'Hello, JDK'
            }
        }
        stage('Staging Area') {
           steps {
                echo 'Hello, JDK'
            }
        }
        stage('Code Deploy') {
           steps {
                echo 'Hello, JDK'
            }
        }
    }
}
