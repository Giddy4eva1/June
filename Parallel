node {
 	// Clean workspace before doing anything
    deleteDir()
    try {
        stage ('Clone') {
        	checkout scm
        }
	stage ('build') {
	parallel 'static' : {
	sh "echo 'try doing the parallel here'"
	},
	'prebuild' : {
	sh "echo 'this is another parallel to be build with others'"
	}
       }
	
	stage ('Deploy') {
            sh "echo 'shell scripts to deploy to server...'"
      	}
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}
