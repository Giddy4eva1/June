node {
 	// Clean workspace before doing anything
    deleteDir()
    try {
        stage ('Clone') {
        	checkout scm
        }
	stage ('practice') {
		sh "echo 'analyse the code'"
		sh "echo 'seek stakeholder approval'"
	}
      	stage ('Deploy') {
            sh "echo 'shell scripts to deploy to server...'"
      	}
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}
